{
	"name": "chihayaburu",
	"version": "2.0.0",
	"description": "Discord bot that automates HoYoLab check-ins.",
	"main": "bot.js",
	"scripts": {
		"format": "npx prettier src --write",
		"lint": "npx eslint --cache src --ext .ts --fix",
		"prebuild": "npm run format && npm run lint",
		"build": "tsc",
		"prod": "npm i && npm run build",
		"start": "node .",
		"quickstart": "tsc && docker compose up -d"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KingWaffleIII/chihayaburu.git"
	},
	"keywords": [],
