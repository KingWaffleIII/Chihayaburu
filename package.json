	"license": "ISC",
	"bugs": {
		"url": "https://github.com/KingWaffleIII/chihayaburu/issues"
	},
	"homepage": "https://github.com/KingWaffleIII/chihayaburu#readme",
	"dependencies": {
		"@rthelolchex/genshininfo_scraper": "^1.1.6",
		"cron": "^2.2.0",
		"discord.js": "^14.8.0",
		"sequelize": "^6.29.3",
		"sqlite3": "^5.1.6"
	},
	"devDependencies": {
		"@types/cron": "^2.0.0"
	}
}
